# Web Scraper Configuration

# Fingerprinting - Enhanced stealth mode
ENABLE_FINGERPRINTING=true

# Optional: Free proxy configuration
USE_FREE_PROXIES=false
PROXY_TIMEOUT=5000
PROXY_COUNTRY=all

# Crawling settings
MAX_CONCURRENCY=2
SEEDS_FILE=seeds.txt

# Browser settings
BROWSER_TYPE=chromium
HEADLESS=true
REQUEST_TIMEOUT=120

# Logging
LOG_LEVEL=INFO
